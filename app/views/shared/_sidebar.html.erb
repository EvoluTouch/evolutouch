<div class="page-sidebar" data-pages="sidebar">

  <div id="appMenu" class="sidebar-overlay-slide from-top">
  </div>

  <div class="sidebar-header">
    <img src="<%= image_path('logo_white.png') %>" alt="logo" class="brand" data-src="<%= image_path('logo_white.png') %>" data-src-retina="<%= image_path('logo_white_2x.png') %>" width="78" height="22 ">
    <div class="sidebar-header-controls">
      <button data-pages-toggle="#appMenu" class="btn btn-xs sidebar-slide-toggle btn-link m-l-20" type="button">
      </button>
      <button data-toggle-pin="sidebar" class="btn btn-link visible-lg-inline" type="button"><i class="fa fs-12"></i>
      </button>
    </div>
  </div>

  <div class="sidebar-menu">
    <ul class="menu-items">

      <li class="m-t-30">
        <a href="<%= root_path %>">
          <span class="title">Accueil</span>
        </a>
        <span class="icon-thumbnail "><i class="pg-home"></i></span>
      </li>

      <li class="">
        <a href="<%= clients_path %>">
          <span class="title">Clients</span>
        </a>
        <span class="icon-thumbnail "><i class="pg-unordered_list"></i></span>
      </li>

      <li class="">
        <a href="<%= root_path %>">
          <span class="title">Mailing</span>
        </a>
        <span class="icon-thumbnail"><i class="pg-mail"></i></span>
      </li>

    </ul>
    <div class="clearfix"></div>

  </div>

</div>
